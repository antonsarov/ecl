    <eStructuralFeatures xsi:type="ecore:EAttribute" name="script" eType="#//EScript"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EDataType" name="EScript" instanceClassName="java.lang.String"/>
  <eClassifiers xsi:type="ecore:EClass" name="DefineCommand" eSuperTypes="#//Command">
    <eStructuralFeatures xsi:type="ecore:EReference" name="message" lowerBound="1"
