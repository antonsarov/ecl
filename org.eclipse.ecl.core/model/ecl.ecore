    <eStructuralFeatures xsi:type="ecore:EAttribute" name="script" eType="#//EScript"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="GetChildren" eSuperTypes="#//Command">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="object" eType="#//Any"/>
