<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://www.eclipse.org/ecl/cc/cvs.ecore"
            class="org.eclipse.ecl.cc.ccvs.CvsPackage"
            genModel="model/cvs.genmodel"/>
   </extension>
   <extension
         point="org.eclipse.ecl.core.scriptlet">
      <scriptlet
            class="org.eclipse.ecl.internal.cc.ccvs.commands.GetCVSLocationsService"
            name="GetCVSLocations"
            namespace="http://www.eclipse.org/ecl/cc/cvs.ecore">
      </scriptlet>
   </extension>
   <extension
         point="org.eclipse.ecl.core.scriptlet">
      <scriptlet
            class="org.eclipse.ecl.internal.cc.ccvs.commands.AddCVSLocationsService"
            name="AddCVSLocations"
            namespace="http://www.eclipse.org/ecl/cc/cvs.ecore">
      </scriptlet>
   </extension>

</plugin>
